{
  "name": "Deshboard",
  "version": "0.1.5",
  "manifest_version": 2,
  "description": "A dashboard full of tools for help developers to get a grateful work day, or night =).",
  "icons": {
    "16": "app/images/icon-16.png",
    "128": "app/images/icon-128.png"
  },
  "app": {
    "background": {
      "scripts": ["app/background.js"]
    }
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "https://ajax.googleapis.com/"
  ]
}
